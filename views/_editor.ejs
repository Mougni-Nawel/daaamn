
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="photos">Document</label>
    <div class="col-sm-8">
      <input type="file" autofocus class="form-control" name="photos" value="<%= model.Photos %>" size=50 />
      <!-- <input autofocus class="form-control" name="photos" value="<%= model.Photos %>" /> -->
      <a href="" download="photos">Télécharger l'image</a>
    </div>
  </div>

  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Nomplante">Nom du devoir</label>
    <div class="col-sm-7">
      <input class="form-control" name="nomplante" value="<%= model.Nomplante %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Proprietaire">Proprietaire</label>
    <div class="col-sm-7">
      <input class="form-control" name="proprietaire" value="<%= model.Proprietaire %>" />
    </div>
  </div>
  
  <div class="form-group row">
    <label class="col-form-label col-sm-2" for="Commentaires">Commentaires</label>
    <div class="col-sm-10">
      <textarea readonly class="form-control" cols="20" id="Commentaires" maxlength="32000" rows="7"><%= model.Commentaires %></textarea>
    </div>
  </div>